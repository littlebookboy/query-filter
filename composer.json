{
    "name": "kblais/query-filter",
    "description": "Easily create filters for your Eloquent model.",
    "license": "MIT",
    "authors": [
        {
            "name": "Killian Blais",
            "email": "contact@killianblais.fr"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/database": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/support": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Kblais\\QueryFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kblais\\QueryFilter\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "^3.2",
        "phpunit/phpunit": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kblais\\QueryFilter\\QueryFilterServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
